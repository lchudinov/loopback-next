{
  "name": "@loopback/graphql",
  "description": "LoopBack's graphql integration",
  "version": "0.11.7",
  "keywords": [
    "LoopBack",
    "GraphQL"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "IBM Corp. and LoopBack contributors",
  "copyright.owner": "IBM Corp. and LoopBack contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/loopbackio/loopback-next.git",
    "directory": "extensions/graphql"
  },
  "engines": {
    "node": "18 || 20 || 22"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-graphql*.tgz dist *.tsbuildinfo package",
    "pretest": "npm run build",
    "integration": "lb-mocha \"dist/__tests__/integration/**/*.js\"",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-graphql*.tgz && tree package && npm run clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "peerDependencies": {
    "@loopback/boot": "^7.0.0",
    "@loopback/core": "^6.0.0"
  },
  "dependencies": {
    "@apollo/cache-control-types": "^1.0.3",
    "@apollo/server": "^4.9.3",
    "@apollo/utils.keyvaluecache": "^3.0.0",
    "@graphql-tools/mock": "^9.0.0",
    "@graphql-tools/utils": "^10.0.1",
    "@loopback/http-server": "^6.0.7",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "debug": "^4.3.7",
    "express": "^4.21.1",
    "graphql": "^16.8.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.13.1",
    "type-graphql": "^2.0.0-beta.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@loopback/boot": "^7.0.7",
    "@loopback/build": "^11.0.6",
    "@loopback/core": "^6.1.4",
    "@types/cors": "^2.8.13",
    "@loopback/eslint-config": "^15.0.4",
    "@loopback/repository": "^7.0.7",
    "@loopback/rest": "^14.0.7",
    "@loopback/testlab": "^7.0.6",
    "@types/debug": "^4.1.12",
    "@types/node": "^16.18.119",
    "@types/ws": "^8.5.5",
    "class-transformer": "^0.5.1"
  }
}
